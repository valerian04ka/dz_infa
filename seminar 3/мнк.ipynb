    "print(np.__version__)\n",
    "def mnk(x,y):\n",
    "    x=np.array(x)\n",
    "    x=np.array(x)\n",
    "    a = (x*y).mean() - x.mean()*y.mean() - x.mean()**2\n",
    "    b = y/mean() - a*x.mean()\n",
    "    return(a,b)\n",
    "x = [1,2,3,4,5]\n",
    "y = [2,3,5,6,7]\n",
    "print(mnk(x,y))\n"
